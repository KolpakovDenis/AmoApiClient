{
  "name": "yoom/amo-api-client",
  "description": "AmoCRM api client by Yoom Team.",
  "keywords": ["AmoCRM", "api client"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Yoom\\": "app/",
      "Test\\": "test/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
